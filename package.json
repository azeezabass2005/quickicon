{
  "name": "quickicon",
  "version": "0.1.0",
  "description": "Convert SVGs to React components instantly from clipboard, files, or URLs",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "quickicon": "./cli.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "cli.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version"
  },
  "keywords": [
    "svg",
    "react",
    "icon",
    "component",
    "cli",
    "converter"
  ],
  "author": "Fola azeezabass2005@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/azeezabass2005/quickicon.git"
  },
  "bugs": {
    "url": "https://github.com/azeezabass2005/quickicon/issues"
  },
  "homepage": "https://github.com/azeezabass2005/quickicon#readme",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "napi": {
    "name": "quickicon",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "optionalDependencies": {
    "@quickicon/linux-x64-gnu": "0.1.0",
    "@quickicon/linux-x64-musl": "0.1.0",
    "@quickicon/linux-arm64-gnu": "0.1.0",
    "@quickicon/linux-arm64-musl": "0.1.0",
    "@quickicon/darwin-x64": "0.1.0",
    "@quickicon/darwin-arm64": "0.1.0",
    "@quickicon/win32-x64-msvc": "0.1.0",
    "@quickicon/win32-arm64-msvc": "0.1.0"
  },
  "engines": {
    "node": ">= 16"
  }
}